			<xs:complexType name="DocumentInfoType">
					<xs:annotation>
					    <xs:documentation>
						This class contains elements related to information about a document. 
					    </xs:documentation>
					</xs:annotation>
				<xs:sequence>
						<xs:element name="Kind" minOccurs="1" maxOccurs="1">
						<xs:complexType>
							<xs:choice>
								<xs:element name="DocumentFromCourtOrTribunal" minOccurs="1" maxOccurs="1">
								<xs:complexType>
									<xs:choice>
										<xs:element name="CourtDecision" minOccurs="1" maxOccurs="1">
										<xs:complexType/>
										</xs:element>
										<xs:element name="DocumentEmanatingFromAPublicProsecutor" minOccurs="1" maxOccurs="1">
										<xs:complexType/>
										</xs:element>
										<xs:element name="DocumentEmanatingFromAClerkOfACourt" minOccurs="1" maxOccurs="1">
										<xs:complexType/>
										</xs:element>
										<xs:element name="DocumentEmanatingFromAJudicialOfficer" minOccurs="1" maxOccurs="1">
										<xs:complexType/>
										</xs:element>
													<xs:element name="Other" type="udt:TextType" minOccurs="1" maxOccurs="1" >
														<xs:annotation>
														    <xs:documentation>
																
														    </xs:documentation>
														</xs:annotation>
													</xs:element>
											
										
									</xs:choice>
								</xs:complexType>
								</xs:element>
								<xs:element name="AdministrativeDocument" minOccurs="1" maxOccurs="1">
								<xs:complexType/>
								</xs:element>
								<xs:element name="NotarialAct" minOccurs="1" maxOccurs="1">
								<xs:complexType/>
								</xs:element>
								<xs:element name="OfficialCertificateOnDocumentSignedByPerson" minOccurs="1" maxOccurs="1">
								<xs:complexType/>
								</xs:element>
													<xs:element name="DocumentDrawnUpByDiplomaticOrConsularAgent" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
														<xs:annotation>
														    <xs:documentation>
																This property contains a checkbox on whether the document is a document drawn up by a diplomatic or consular agent.
														    </xs:documentation>
														</xs:annotation>
													</xs:element>
											
								
							</xs:choice>
						</xs:complexType>
						</xs:element>
										<xs:element name="IssueDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
											<xs:annotation>
											    <xs:documentation>
													This property contains the issue date of the document.
											
											    </xs:documentation>
											</xs:annotation>
										</xs:element>
								
						
									<xs:element name="ReferenceNumber " type="udt:TextType" minOccurs="1" maxOccurs="1" >
											
											    					<xs:annotation>
											
											    					    <xs:documentation>
											
											    							This property contains the reference number of the document.
											
											    					    </xs:documentation>
											
											    					</xs:annotation>
											
											    				</xs:element>
											
											    		
											
											    	
											
											    
				</xs:sequence>
				
			</xs:complexType>
		
		
			
			
			<xs:complexType name="AbsenceOfCriminalRecordType">
					<xs:annotation>
					    <xs:documentation>
						This class contains elements related to the absence of a criminal record of a person. 
					    </xs:documentation>
					</xs:annotation>
				<xs:sequence>
								<xs:element name="Person" type="pdc:AbsenceOfCriminalRecordPersonType" minOccurs="1" maxOccurs="1"/>
						
									<xs:element name="IdentityNumber" type="udt:TextType" minOccurs="1" maxOccurs="1" >
										<xs:annotation>
										    <xs:documentation>
												This property contains the identity number. 
										    </xs:documentation>
										</xs:annotation>
									</xs:element>
							
						
						<xs:element name="IDTypeAndNo" minOccurs="1" maxOccurs="1">
						<xs:complexType>
							<xs:choice>
											<xs:element name="NationalIdentityCardNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
												<xs:annotation>
												    <xs:documentation>
														This property contains the number of the national identity card.
												    </xs:documentation>
												</xs:annotation>
											</xs:element>
									
								
											<xs:element name="PassportNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
												<xs:annotation>
												    <xs:documentation>
														This property contains the passport number.
												    </xs:documentation>
												</xs:annotation>
											</xs:element>
									
								
											<xs:element name="DrivingLicenceNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
												<xs:annotation>
												    <xs:documentation>
														This property contains the driving licence number.
												    </xs:documentation>
												</xs:annotation>
											</xs:element>
									
								
										<xs:element name="OtherIDType" type="OtherIDTypeType" minOccurs="1" maxOccurs="1" >
											<xs:annotation>
											    <xs:documentation>
													This property contains the name of another possible ID type.
											    </xs:documentation>
											</xs:annotation>
										</xs:element>
								
							</xs:choice>
						</xs:complexType>
						</xs:element>
											<xs:element name="AbsenceOfACriminalRecord" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
												<xs:annotation>
												    <xs:documentation>
														This property contains the boolean value (true or false) regarding the absence of a criminal record.
												    </xs:documentation>
												</xs:annotation>
											</xs:element>
									
						
					
				</xs:sequence>
				
			</xs:complexType>
		
		
			
