	<:complexType name="CapacityToEnterRegisteredPartnershipType">
		<:annotation>
		    <:documentation xml:lang="">
			
		    </:documentation>
		</:annotation>
		<:complexContent>
			<:extension base="forms:CapacityToEnterRegisteredPartnershipType">
				<:sequence>
					<:element name="LastMarriageOrRegisteredPartnership" type="LastMarriageOrRegisteredPartnershipType" minOccurs="1" maxOccurs="1" >
						<:annotation>
						    <:documentation xml:lang="">
								This property refers to the class LastMarriageOrRegisteredPartnership.
						    </:documentation>
						</:annotation>
					</:element>
					
				</:sequence>
			</:extension>
		</:complexContent>
	</:complexType>
	<:complexType name="CapacityToMarryType">
		<:annotation>
		    <:documentation xml:lang="">
			
		    </:documentation>
		</:annotation>
		<:complexContent>
			<:extension base="forms:CapacityToMarryType">
				<:sequence>
					<:element name="LastMarriageOrRegisteredPartnership" type="LastMarriageOrRegisteredPartnershipType" minOccurs="1" maxOccurs="1" >
						<:annotation>
						    <:documentation xml:lang="">
								
						    </:documentation>
						</:annotation>
					</:element>
					
				</:sequence>
			</:extension>
		</:complexContent>
	</:complexType>
	<:complexType name="DocumentInfoType">
		<:annotation>
		    <:documentation xml:lang="">
			
		    </:documentation>
		</:annotation>
		<:complexContent>
			<:extension base="forms:DocumentInfoType">
				<:sequence>
					<:element name="CourtOfIssue" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
					
				</:sequence>
			</:extension>
		</:complexContent>
	</:complexType>
	<:complexType name="DomicileAndOrResidenceType">
		<:annotation>
		    <:documentation xml:lang="">
			
		    </:documentation>
		</:annotation>
		<:complexContent>
			<:extension base="forms:DomicileAndOrResidenceType">
				<:sequence>
					<:element name="SecondaryResidence" type="pdc:AddressType" minOccurs="1" maxOccurs="1"/>
					
				</:sequence>
			</:extension>
		</:complexContent>
	</:complexType>
	<:complexType name="FormerSpouseOrPartnerType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the former spouse or partner of the person concerned by the form.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="FormerSpouseOrPartner" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="FormerSpouseOrPartnerChoiceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the choice of marital status.
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			<:element name="FormerSpouseOrPartner" type="FormerSpouseOrPartnerType" minOccurs="1" maxOccurs="1" >
				<:annotation>
				    <:documentation xml:lang="">
						
				    </:documentation>
				</:annotation>
			</:element>
			
		</:choice>
	</:complexType>
	<:complexType name="IDTypeAndNoType">
		<:annotation>
		    <:documentation xml:lang="">
			
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			<:element name="IssuingAuthority" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
			<:element name="IssuingState" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
			
		</:choice>
	</:complexType>
	<:complexType name="LastMarriageOrRegisteredPartnershipType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains information about the last marriage or registered partnership of the person concerned by the form.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="DissolvedBy" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
			<:element name="DissolvedIn" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="MaritalStatusType"/>
	<:complexType name="PersonType">
		<:annotation>
		    <:documentation xml:lang="">
			
					Stereotype cvABIE
		    </:documentation>
		</:annotation>
		<:complexContent>
			<:extension base="pdc:PersonType">
				<:sequence>
					<:element name="LastMarriageOrRegisteredPartnership" minOccurs="1" maxOccurs="1" type="MaritalStatusType">
					</:element>
					<:element name="FormerSpouseOrPartner" minOccurs="1" maxOccurs="1" type="FormerSpouseOrPartnerChoiceType">
					</:element>
					<:element name="LastPlaceOfResidence" type="pdc:AddressType" minOccurs="1" maxOccurs="1"/>
					<:element name="Mother" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
					<:element name="Father" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
					
				</:sequence>
			</:extension>
		</:complexContent>
	</:complexType>
	<:complexType name="TravelDocumentChoiceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the choice of travel document.
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			<:element name="TravelDocument" minOccurs="1" maxOccurs="1" type="IDTypeAndNoType">
			</:element>
			
		</:choice>
	</:complexType>
	</:schema>
