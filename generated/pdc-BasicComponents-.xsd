<?xml version="1.0" encoding="UTF-8"?>
<:schema
	targetNamespace=""
	xmlns=""
	xmlns:cvb=""	
	xmlns:udt=""	
	xmlns:="http://www.w3.org/2001/XMLSchema"	
	elementFormDefault="qualified"
	attributeFormDefault="unqualified"
	version="">
	<:annotation>	
		<:documentation xml:lang="en">
	         
	        Copyright (C) European Union, 2017
	        Licensed under the 
	        Original authors:
	         (),
	         (),
	         ()
		</:documentation>
	</:annotation>
	<!-- ===== Imports======================================== -->
	<:import namespace=""
			   schemaLocation=""/>
	<:import namespace=""
			   schemaLocation=""/>
	<!-- ===================================================== -->
	<!-- ============================================================ -->
	<!-- Address-->
	<!-- uses  Address --> 
	<!-- ============================================================ -->
	
	<:complexType name="AddressCountry">
		<:simpleContent>
			<:restriction base="cvb:AddressAdminUnitLocationOne">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<:complexType name="AddressPlace">
		<:simpleContent>
			<:restriction base="cvb:AddressPostName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<:complexType name="AddressPostalCode">
		<:simpleContent>
			<:restriction base="cvb:AddressPostName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<:complexType name="AddressStreetPOBox">
		<:simpleContent>
			<:restriction base="cvb:AddressThoroughfare">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<!-- ============================================================ -->
	<!-- Authority-->
	 <!-- uses LegalEntity --> 
	<!-- ============================================================ -->
	
	<:complexType name="AuthorityDesignation">
		<:simpleContent>
			<:restriction base="cvb:LegalEntityLegalName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<!-- ============================================================ -->
	<!-- Country-->
	<!-- uses  Location --> 
	<!-- ============================================================ -->
	
	<:complexType name="CountryName">
		<:simpleContent>
			<:restriction base="cvb:LocationGeographicName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<!-- ============================================================ -->
	<!-- Nationality-->
	 <!-- uses Jurisdiction --> 
	<!-- ============================================================ -->
	
	<:complexType name="NationalityName">
		<:simpleContent>
			<:restriction base="cvb:JurisdictionName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<!-- ============================================================ -->
	<!-- Person-->
	<!-- uses  Person --> 
	<!-- ============================================================ -->
	
	<:complexType name="PersonDateOfBirth">
		<:simpleContent>
			<:extension base="udt:DateType" />
		</:simpleContent>
	</:complexType>
	
	
	<:complexType name="PersonDateOfDeath">
		<:simpleContent>
			<:extension base="udt:DateType" />
		</:simpleContent>
	</:complexType>
	
	
	<:complexType name="PersonForename">
		<:simpleContent>
			<:restriction base="cvb:PersonGivenName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<:complexType name="PersonSurname">
		<:simpleContent>
			<:restriction base="cvb:PersonFamilyName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<:complexType name="PersonSurnameAtBirth">
		<:simpleContent>
			<:restriction base="cvb:PersonBirthName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	<!-- ============================================================ -->
	<!-- Place-->
	<!-- uses  Location --> 
	<!-- ============================================================ -->
	
	<:complexType name="PlaceName">
		<:simpleContent>
			<:restriction base="cvb:LocationGeographicName">
				<:minLength value="0"  />
				<:maxLength value="255" />
			</:restriction>
		</:simpleContent>
	</:complexType>
	
	
</:schema>
