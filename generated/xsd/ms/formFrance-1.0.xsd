<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
	targetNamespace="http://e-documents.semic.eu/publicdocuments/1.0/ns/forms/france"
	xmlns="http://e-documents.semic.eu/publicdocuments/1.0/ns/forms/france"
	xmlns:pdc="http://e-documents.semic.eu/publicdocuments/1.0/ns/pdc"	
	xmlns:forms="http://e-documents.semic.eu/publicdocuments/1.0/ns/forms"	
	xmlns:udt="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"	
	xmlns:cls="http://e-documents.semic.eu/publicdocuments/1.0/ns/codelist/sex"	
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"	
	elementFormDefault="qualified"
	attributeFormDefault="unqualified"
	version="1.0">
	<xsd:annotation>	
		<xsd:documentation xml:lang="en">
	        Public Documents XML schema 1.0
	        Copyright (C) European Union, 2017
	        Licensed under the ISA Open Metadata Licence
	        Original authors:
	        Suzanne Wigard (suzanne.wigard@ec.europa.eu),
	        Emidio Stani (emidio.stani@be.pwc.com),
	        Ioana Novacean (ioana.novacean@be.pwc.com)
		</xsd:documentation>
	</xsd:annotation>
	<!-- ===== Imports======================================== -->
	<xsd:import namespace="http://e-documents.semic.eu/publicdocuments/1.0/ns/pdc"
			   schemaLocation="../../xsd/common/core/pdc-AggregatedComponents-1.0.xsd"/>
	<xsd:import namespace="http://e-documents.semic.eu/publicdocuments/1.0/ns/forms"
			   schemaLocation="../../xsd/common/core/forms-1.0.xsd"/>
	<xsd:import namespace="urn:oasis:names:specification:ubl:schema:xsd:UnqualifiedDataTypes-2"
			   schemaLocation="../../xsd/external/UBL-2.1/xsdrt/common/UBL-UnqualifiedDataTypes-2.1.xsd"/>
	<xsd:import namespace="http://e-documents.semic.eu/publicdocuments/1.0/ns/codelist/sex"
			   schemaLocation="../../xsd/common/codelist/cls-codelist-Sex-1.0.xsd"/>
	<!-- ===================================================== -->
	<xsd:complexType name="AbsenceOfCriminalRecordType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the absence of a criminal record of a person. 
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="forms:AbsenceOfCriminalRecordType">
				<xsd:sequence>
					<xsd:element name="Person" type="pdc:AbsenceOfCriminalRecordPersonType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="IdentityNumber" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the identity number. 
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="IDTypeAndNo" type="forms:IDTypeAndNoType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="AbsenceOfACriminalRecord" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the boolean value (true or false) regarding the absence of a criminal record.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PublicDocumentVerificationWebsite" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the URL of the public document verification website.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BirthType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="forms:BirthType">
				<xsd:sequence>
					<xsd:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="KeyReferences" minOccurs="1" maxOccurs="1" type="KeyReferencesChoiceType">
					</xsd:element>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="BirthPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="pdc:PersonType">
				<xsd:sequence>
					<xsd:element name="Surname" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the person's surname.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="Forename" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the forename (first name or given name) of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="DateOfBirth" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the date of birth of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PlaceOfBirth" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="CountryOfBirth" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property refers to the class Sex.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="BirthTime" type="udt:TimeType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the time of birth of the person concerned by the form.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="DeclaredByParent" minOccurs="1" maxOccurs="1" type="DeclaredByParentChoiceType">
					</xsd:element>
					<xsd:element name="DeclaredByOther" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CapacityToMarryType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the capacity to marry of a couple.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="forms:CapacityToMarryType">
				<xsd:sequence>
					<xsd:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="IntendedSpouse" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="CanMarry" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="MarriagePublished" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the boolean value of whether there is a marriage published concerning the person concerned by the form. 
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="AdditionalInformation" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This form contains additional information. 
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConsentNeededType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the consent needed for a marriage.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="ConsentOfMotherOrFatherOrAncestorOfSpouse" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the boolean value of whether the mother, father or other ancestor of the person seeking to get married has given consent.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="ConsentNeededChoiceType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the choice of whether consent is needed.
					Stereotype xsdChoice
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="ConsentNeeded" type="ConsentNeededType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property refers to the class ConsentNeeded.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NoConsentNeeded" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This class relates to the class NoConsentNeeded.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="DeathType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the death of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="forms:DeathType">
				<xsd:sequence>
					<xsd:element name="Person" type="pdc:DeathPersonType" minOccurs="1" maxOccurs="1"/>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DeathDeclarerType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the person declaring a death. 
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="RelationshipToDeceased" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This class contains information about the relationship to the deceased of the person declaring the death. 
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="DeathPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the death of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="pdc:PersonType">
				<xsd:sequence>
					<xsd:element name="Surname" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the surname (i.e. the last name or family name) of the person. 
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="Forename" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the forename (i.e. the first name or given name) of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="DateOfBirth" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the date of birth of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PlaceOfBirth" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="CountryOfBirth" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="DateOfDeath" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the date of death of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PlaceOfDeath" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="CountryOfDeath" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property refers to the class Sex.
						
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LastPlaceOfResidence" type="cva:AddressType" minOccurs="1" maxOccurs="1" >
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation>
						
						    			
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    </xsd:element>
						
						    <xsd:element name="LastMarriageOrRegisteredPartnership" type="forms:MaritalStatusType" minOccurs="1" maxOccurs="1"/>
						
						    <xsd:element name="FormerSpouseOrPartner" type="FormerSpouseOrPartnerType" minOccurs="1" maxOccurs="1" >
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation>
						
						    			
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    </xsd:element>
						
						    <xsd:element name="FormerSpouseOrPartner" minOccurs="1" maxOccurs="1" type="FormerSpouseOrPartnerChoiceType">
						
						    </xsd:element>
						
						    <xsd:element name="TimeOfDeath" type="udt:TimeType" minOccurs="1" maxOccurs="1" >
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation>
						
						    			This property contains the time of death of the person concerned by the form. 
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    </xsd:element>
						
						    
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DeclaredByParentChoiceType">
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation xml:lang = "en">
						
						    		This class contains elements related to the choice of which parent declared the person concerned by the form.
						
						    				Stereotype xsdChoice
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    	<xsd:choice>
						
						    		<xsd:element name="DeclaredByFather" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						
						    			<xsd:annotation>
						
						    			    <xsd:documentation>
						
						    					This property refers to the class DeclaredByFather.
						
						    			    </xsd:documentation>
						
						    			</xsd:annotation>
						
						    		</xsd:element>
						
						    		<xsd:element name="DeclaredByMother" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						
						    			<xsd:annotation>
						
						    			    <xsd:documentation>
						
						    					This property refers to the class DeclaredByMother.
						
						    			    </xsd:documentation>
						
						    			</xsd:annotation>
						
						    		</xsd:element>
						
						    		<xsd:element name="DeclaredByFatherAndMother" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						
						    			<xsd:annotation>
						
						    			    <xsd:documentation>
						
						    					This property relates to the class DeclaredByFatherAndMother.
						
						    			    </xsd:documentation>
						
						    			</xsd:annotation>
						
						    		</xsd:element>
						
						    		
						
						    	</xsd:choice>
						
						    </xsd:complexType>
	<xsd:complexType name="DivorceType">
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation xml:lang = "en">
						
						    		
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    	<xsd:sequence> 
						
						    		<xsd:element name="DivorcePerson" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
						
						    		<xsd:element name="DivorceDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						
						    			<xsd:annotation>
						
						    			    <xsd:documentation>
						
						    					
						
						    			    </xsd:documentation>
						
						    			</xsd:annotation>
						
						    		</xsd:element>
						
						    		<xsd:element name="NatureOfDivorceDecision" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						
						    			<xsd:annotation>
						
						    			    <xsd:documentation>
						
						    					
						
						    			    </xsd:documentation>
						
						    			</xsd:annotation>
						
						    		</xsd:element>
						
						    		<xsd:element name="DivorceAuthority" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
						
						    		
						
						    	</xsd:sequence>
						
						    	
						
						    </xsd:complexType>
	<xsd:complexType name="DocumentInfoType">
						
						    	<xsd:annotation>
						
						    	    <xsd:documentation xml:lang = "en">
						
						    		This class contains elements related to information about a document. 
						
						    	    </xsd:documentation>
						
						    	</xsd:annotation>
						
						    	<xsd:complexContent>
						
						    		<xsd:extension base="forms:DocumentInfoType">
						
						    			<xsd:sequence>
						
						    				<xsd:element name="Kind" type="forms:DocumentKindChoiceType" minOccurs="1" maxOccurs="1"/>
						
						    				<xsd:element name="IssueDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						
						    					<xsd:annotation>
						
						    					    <xsd:documentation>
						
						    							This property contains the issue date of the document.
						
						    					
						
						    					    </xsd:documentation>
						
						    					</xsd:annotation>
						
						    				</xsd:element>
						
						    				<xsd:element name="ReferenceNumber" type="forms:ReferenceNumberType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    <xsd:element name="CertificateOfficial" type="pdc:OfficialType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    <xsd:element name="IssueTime" type="udt:TimeType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation>
						
						    					
						
						    					    			This property contains the time of issue of the form.
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    </xsd:element>
						
						    					
						
						    					    <xsd:element name="DateOfDecision" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation>
						
						    					
						
						    					    			This property contains the date the decision mentioned in the form was taken. 
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    </xsd:element>
						
						    					
						
						    					    <xsd:element name="NatureOfDecision" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation>
						
						    					
						
						    					    			This property contains the nature of the decision mentioned in the form.
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    </xsd:element>
						
						    					
						
						    					    <xsd:element name="DecisionAuthority" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    <xsd:element name="DateOfAuthorisation" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation>
						
						    					
						
						    					    			This property contains the date when an authorisation was given. 
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    </xsd:element>
						
						    					
						
						    					    <xsd:element name="AuthorisationAuthority" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    <xsd:element name="OtherMentions" type="udt:TextType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation>
						
						    					
						
						    					    			This property contains other mentions. 
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    </xsd:element>
						
						    					
						
						    					    
						
						    			</xsd:sequence>
						
						    		</xsd:extension>
						
						    	</xsd:complexContent>
						
						    </xsd:complexType>
	<xsd:complexType name="FormerSpouseOrPartnerType">
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation xml:lang = "en">
						
						    					
						
						    					    		This class contains elements related to the former spouse or partner of the person concerned by the form. 
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    	<xsd:sequence> 
						
						    					
						
						    					    		<xsd:element name="FormerSpouseOrPartner" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    		
						
						    					
						
						    					    	</xsd:sequence>
						
						    					
						
						    					    	
						
						    					
						
						    					    </xsd:complexType>
	<xsd:complexType name="FormerSpouseOrPartnerChoiceType">
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation xml:lang = "en">
						
						    					
						
						    					    		This class contains elements related to the choice of whether the person concerned by the form has a former spouse or partner.
						
						    					
						
						    					    				Stereotype xsdChoice
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    	<xsd:choice>
						
						    					
						
						    					    		<xsd:element name="FormerSpouseOrPartner" type="FormerSpouseOrPartnerType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    			<xsd:annotation>
						
						    					
						
						    					    			    <xsd:documentation>
						
						    					
						
						    					    					
						
						    					
						
						    					    			    </xsd:documentation>
						
						    					
						
						    					    			</xsd:annotation>
						
						    					
						
						    					    		</xsd:element>
						
						    					
						
						    					    		<xsd:element name="NoFormerOrPartnerSpouse" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    			<xsd:annotation>
						
						    					
						
						    					    			    <xsd:documentation>
						
						    					
						
						    					    					This property relates to the class NoFormerSpouseOrPartner.
						
						    					
						
						    					    			    </xsd:documentation>
						
						    					
						
						    					    			</xsd:annotation>
						
						    					
						
						    					    		</xsd:element>
						
						    					
						
						    					    		
						
						    					
						
						    					    	</xsd:choice>
						
						    					
						
						    					    </xsd:complexType>
	<xsd:complexType name="KeyReferencesChoiceType">
						
						    					
						
						    					    	<xsd:annotation>
						
						    					
						
						    					    	    <xsd:documentation xml:lang = "en">
						
						    					
						
						    					    		This class contains elements related to the choice of marital status.
						
						    					
						
						    					    				Stereotype xsdChoice
						
						    					
						
						    					    	    </xsd:documentation>
						
						    					
						
						    					    	</xsd:annotation>
						
						    					
						
						    					    	<xsd:choice>
						
						    					
						
						    					    		<xsd:element name="Marriage" type="forms:MarriageType" minOccurs="1" maxOccurs="1"/>
						
						    					
						
						    					    		<xsd:element name="Divorce" type="DivorceType" minOccurs="1" maxOccurs="1" >
						
						    					
						
						    					    			<xsd:annotation>
						
						    					
						
						    					    			    <xsd:documentation>
						
						    					
						
						    					    					
						
						    					
						
						    					    			    </xsd:documentation>
						
						    					
						
						    					    			</xsd:annotation>
						
						    					
						
						    					    		</xsd:element>
						
						    					
						
						    					    		<xsd:element name="LegalSeparation" type="LegalSeparationType" minOccurs="1" maxOccurs="1" >
						
						    					
						
