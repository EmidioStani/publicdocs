																					    <xsd:documentation xml:lang="en">
																							This property can contain any type of information.
																					    </xsd:documentation>
																					</xsd:annotation>
																				</xsd:element>
																				
												</xsd:sequence>
												
											</xsd:complexType>
<xsd:complexType name="HypothesisChoiceType">
																						<xsd:annotation>
																						    <xsd:documentation xml:lang="en">
																							This choice class contains the options relating to the type of transcription of a document.
																									Stereotype deathForm
																									Stereotype xsdChoice
																						    </xsd:documentation>
																						</xsd:annotation>
																						<xsd:choice>
																											<xsd:element name="FromJudgmentOfDeath" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
																											<xsd:annotation>
																											    <xsd:documentation xml:lang="en">
																													This property refers to the option "Document transcribed from a judgment of death".
																											    </xsd:documentation>
																											</xsd:annotation>
																										</xsd:element>
																														<xsd:element name="FromJudgmentOfAbsence" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
																																<xsd:annotation>
																																    <xsd:documentation xml:lang="en">
																																		This property refers to the option "Document transcribed from a judgment of absence".
																																    </xsd:documentation>
																																</xsd:annotation>
																															</xsd:element>
																																			<xsd:element name="FromNationalCourtDecision" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
																																					<xsd:annotation>
																																					    <xsd:documentation xml:lang="en">
																																							This property refers to the option "Document transcribed from a national court decision".
																																					    </xsd:documentation>
																																					</xsd:annotation>
																																				</xsd:element>
																																								<xsd:element name="FromForeignCivilStatusDocument" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
																																										<xsd:annotation>
																																										    <xsd:documentation xml:lang="en">
																																												This property refers to the option "Document transcribed from a foreign civil status document".
																																										    </xsd:documentation>
																																										</xsd:annotation>
																																									</xsd:element>
																																													<xsd:element name="Other" type="udt:TextType" minOccurs="1" maxOccurs="1" >
																																															<xsd:annotation>
																																															    <xsd:documentation xml:lang="en">
																																																	This property refers to the option "Other" and if selected must be specified.
																																															    </xsd:documentation>
																																															</xsd:annotation>
																																														</xsd:element>
																																																		<xsd:element name="FromForeignCourtDecision" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
																																																				<xsd:annotation>
																																																				    <xsd:documentation xml:lang="en">
																																																						This property refers to the option "Act transcribed from a foreign court decision duly exequaturated to Luxembourg". 
																																																				    </xsd:documentation>
																																																				</xsd:annotation>
																																																			</xsd:element>
																																																			
																						</xsd:choice>
																					</xsd:complexType>
