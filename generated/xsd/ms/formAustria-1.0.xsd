	<xsd:complexType name="AbsenceOfCriminalRecordType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the absence of a criminal record of a person. 
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:AbsenceOfCriminalRecordPersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="IdentityNumber" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the identity number. 
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IDTypeAndNo" type="forms:IDTypeAndNoType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="AbsenceOfACriminalRecord" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the boolean value (true or false) regarding the absence of a criminal record.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="AustrianCitizenNoDocumentType"/>
	<xsd:complexType name="BirthType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="BirthPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="pdc:PersonType">
				<xsd:sequence>
					<xsd:element name="Surname" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the person's surname.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="Forename" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the forename (first name or given name) of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="DateOfBirth" type="udt:DateType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property contains the date of birth of the person.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PlaceOfBirth" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="CountryOfBirth" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation>
								This property refers to the class Sex.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CapacityToEnterRegisteredPartnershipType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the capacity to enter into a registered partnership of a couple.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="IntendedPartner" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="CanEnterRegisteredPartnership" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="LastMarriageOrRegisteredPartnership" type="LastMarriageOrRegisteredPartnershipType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property refers to the class LastMarriageOrRegisteredPartnership.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="CapacityToMarryType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the capacity to marry of a couple.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="IntendedSpouse" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="CanMarry" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="LastMarriageOrRegisteredRelationship" type="LastMarriageOrRegisteredPartnershipType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="DeathType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to the death of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Person" type="pdc:DeathPersonType" minOccurs="1" maxOccurs="1"/>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="DeathPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang = "en">
			This class contains elements related to a person.
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Surname" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the person's last name.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="SurnameAtBirth" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the person's last name at the time of his/her birth.
				
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="Forename" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    			This property contains the person's first name or given name.
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    </xsd:element>	
				
				    <xsd:element name="DateOfBirth" type="udt:DateType" minOccurs="0" maxOccurs="1" >
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    			This property contains the person's date of birth.
				
				    	
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    </xsd:element>
				
				    <xsd:element name="PlaceOfBirth" type="pdc:PlaceType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="CountryOfBirth" type="pdc:CountryType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="DateOfDeath" type="udt:DateType" minOccurs="0" maxOccurs="1" >
				
				    	
				
				    	    	<xsd:annotation>
				
				    	
				
				    	    	    <xsd:documentation>
				
				    	
				
				    	    			This property contains the person's date of death (if applicable).
				
				    	
				
				    	    	    </xsd:documentation>
				
				    	
				
				    	    	</xsd:annotation>
				
				    	
				
				    	    </xsd:element>
				
				    	
				
				    	    <xsd:element name="PlaceOfDeath" type="pdc:PlaceType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="CountryOfDeath" type="pdc:CountryType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="Sex" type="cls:SexType" minOccurs="0" maxOccurs="1" >
				
				    	
				
				    	    	<xsd:annotation>
				
				    	
				
				    	    	    <xsd:documentation>
				
				    	
				
				    	    			This property contains the sex of the person
				
				    	
				
				    	    	    </xsd:documentation>
				
				    	
				
				    	    	</xsd:annotation>
				
				    	
				
				    	    </xsd:element>
				
				    	
				
				    	    <xsd:element name="Nationality" type="pdc:NationalityType" minOccurs="0" maxOccurs="unbounded"/>
				
				    	
				
				    	    <xsd:element name="Domicile" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="Residence" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    <xsd:element name="SurnameBefore" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				
				    	
				
				    	    	<xsd:annotation>
				
				    	
				
				    	    	    <xsd:documentation>
				
				    	
				
				    	    			This property contains the past surname of the person (if applicable)
				
				    	
				
				    	    	    </xsd:documentation>
				
				    	
				
				    	    	</xsd:annotation>
				
				    	
				
				    	    </xsd:element>	
				
				    	
				
				    	    <xsd:element name="SurnameAfter" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				
				    	
				
				    	    	<xsd:annotation>
				
				    	
				
				    	    	    <xsd:documentation>
				
				    	
				
				    	    			This property contains the future surname of the person (if applicable)
				
				    	
				
				    	    	    </xsd:documentation>
				
				    	
				
				    	    	</xsd:annotation>
				
				    	
				
				    	    </xsd:element>	
				
				    	
				
				    	    <xsd:element name="PreviousSurname" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				
				    	
				
				    	    	<xsd:annotation>
				
				    	
				
				    	    	    <xsd:documentation>
				
				    	
				
				    	    			This property contains the previous surname of the person (if applicable)
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:documentation>
				
				    	
				
				    	    	</xsd:annotation>
				
				    	
				
				    	    </xsd:element>	
				
				    	
				
				    	    <xsd:element name="PreviousForename" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property contains the previous forename (first name) of the person (if applicable). 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>	
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="OtherNames" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property contains other names used by the concerned person.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="AcademicQualificationsOrProfessionalTitles" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property contains academic qualifications or professional titles held by the concerned person. 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="Father" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="Mother" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="LastMarriageOrRegisteredPartnership" type="MaritalStatusType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property relates to the class MaritalStatus.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="FormerSpouseOrPartner" minOccurs="1" maxOccurs="1" type="FormerSpouseOrPartnerChoiceType">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="RegistrationOfBirth" minOccurs="1" maxOccurs="1" type="ReferenceNumberType">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="AcademicTitlePrefix" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property contains the academic title prefix of the concerned person. 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="AcademicTitleSuffix" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property contains the academic title suffix of the concerned person. 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    <xsd:element name="LastPlaceOfResidence" type="cva:AddressType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			This property relates to the class Address.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="DocumentInfoType">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    <xsd:documentation xml:lang = "en">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		This class contains elements related to information about a document. 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	<xsd:sequence> 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		<xsd:element name="Kind" type="KindType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    					This property refers to the class DocumentKindChoice.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		</xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		<xsd:element name="IssueDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    					This property contains the issue date of the document.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		</xsd:element>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    		<xsd:element name="ReferenceNumber" type="forms:ReferenceNumberType" minOccurs="1" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    <xsd:element name="CourtOfIssue" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	</xsd:sequence>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    	
				
				    	
				
				    	    	
				
				    	
				
				    	    	    </xsd:complexType>
	<xsd:complexType name="DomicileAndOrResidenceType">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    	<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    	    <xsd:documentation xml:lang = "en">
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    		This class contains elements related to the domicile and/or residence of a person.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    	    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    	</xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    	<xsd:sequence> 
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    		<xsd:element name="ConcernedPerson" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    		<xsd:element name="Domicile" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    		<xsd:element name="Residence" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    		<xsd:element name="RegisteredSince" type="udt:DateType" minOccurs="1" maxOccurs="1" >
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    			<xsd:annotation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    			    <xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    					This property contains the date on which the domicile and/or residence of the person concerned by the form was registered.
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			    			    </xsd:documentation>
				
				    	
				
				    	    	
				
				    	
				
				    	    	    			
				
				    	
				
				    	    	
				
				    	
				
