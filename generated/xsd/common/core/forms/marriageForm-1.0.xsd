							</xsd:annotation>
						</xsd:element>
														<xsd:element name="PlaceOfMarriage" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
														
						
	</xsd:sequence>
	
</xsd:complexType>

<xsd:element name="FrmMarriage" type="FrmMarriageType"/>
