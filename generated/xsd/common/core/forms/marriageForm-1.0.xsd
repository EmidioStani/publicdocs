<xsd:complexType name="MarriageType">
	<xsd:annotation>
	    <xsd:documentation xml:lang="en">
		This class contains elements related to a marriage.
				Stereotype marriageForm
	    </xsd:documentation>
	</xsd:annotation>
	<xsd:sequence> 
										<xsd:element name="SpouseA" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
										
										<xsd:element name="SpouseB" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
										
