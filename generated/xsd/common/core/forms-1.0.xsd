	<xsd:complexType name="AbsenceOfCriminalRecordType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the absence of a criminal record of a person. 
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="pdc:AbsenceOfCriminalRecordPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element name="IdentityNumber" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the identity number. 
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			
				
			<xsd:element name="IDTypeAndNo" minOccurs="1" maxOccurs="1" type="IDTypeAndNoType">
			</xsd:element>
			
			
			<xsd:element name="AbsenceOfACriminalRecord" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the boolean value (true or false) regarding the absence of a criminal record.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			
			
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
	
	<xsd:complexType name="AdministrativeDocumentChoiceType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the choice between the different types of administrative documents. 
					Stereotype xsdChoice
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
				
			<xsd:element name="Certificate" minOccurs="1" maxOccurs="1" type="CertificateType">
			</xsd:element>
			
			
				
			<xsd:element name="ExtractFromTheCivilStatusRegister" minOccurs="1" maxOccurs="1" type="ExtractFromTheCivilStatusRegisterType"/>
			
			
				
			<xsd:element name="ExtractFromThePopulationRegister" minOccurs="1" maxOccurs="1" type="ExtractFromThePopulationRegisterType">
			</xsd:element>
			
			
				
			<xsd:element name="VerbatimCopyOfCivilStatusRecords" minOccurs="1" maxOccurs="1" type="VerbatimCopyOfCivilStatusRecordsType">
			</xsd:element>
			
			
			<xsd:element name="Other" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the string value referring to another type of administrative document. 
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			
			
		</xsd:choice>
	</xsd:complexType>
	
	
	
	
	
	<xsd:complexType name="BirthType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
	
	<xsd:complexType name="CapacityToEnterRegisteredPartnershipType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the capacity to enter into a registered partnership of a couple.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element name="IntendedPartner" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element name="CanEnterRegisteredPartnership" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			
			
			
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
	
	<xsd:complexType name="CapacityToMarryType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the capacity to marry of a couple.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element name="IntendedSpouse" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element name="CanMarry" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			
			
			
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
	<xsd:complexType name="CertificateType"/>
	
	
	
	
	<xsd:complexType name="CourtDecisionType"/>
	
	
	
	
	
	<xsd:complexType name="DeathType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the death of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="pdc:DeathPersonType" minOccurs="1" maxOccurs="1"/>
			
			
			
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
	<xsd:complexType name="DocumentDrawnUpByDiplomaticOrConsularAgentType"/>
	
	
	
	
	<xsd:complexType name="DocumentEmanatingFromAClerkOfACourtType"/>
	
	
	
	
	<xsd:complexType name="DocumentEmanatingFromAJudicialOfficerType"/>
	
	
	
	
	<xsd:complexType name="DocumentEmanatingFromAPublicProsecutorType"/>
	
	
	
	
	
	<xsd:complexType name="DocumentFromCourtOrTribunalChoiceType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to the choice between different types of documents emanating from a court or tribunal.
					Stereotype xsdChoice
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
				
			<xsd:element name="CourtDecision" minOccurs="1" maxOccurs="1" type="CourtDecisionType">
			</xsd:element>
			
			
				
			<xsd:element name="DocumentEmanatingFromAPublicProsecutor" minOccurs="1" maxOccurs="1" type="DocumentEmanatingFromAPublicProsecutorType">
			</xsd:element>
			
			
				
			<xsd:element name="DocumentEmanatingFromAClerkOfACourt" minOccurs="1" maxOccurs="1" type="DocumentEmanatingFromAClerkOfACourtType">
			</xsd:element>
			
			
				
			<xsd:element name="DocumentEmanatingFromAJudicialOfficer" minOccurs="1" maxOccurs="1" type="DocumentEmanatingFromAJudicialOfficerType">
			</xsd:element>
			
			
			<xsd:element name="Other" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the string value referring to another type of document coming from court or tribunal.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			
			
		</xsd:choice>
	</xsd:complexType>
	
	
	
	
	
	<xsd:complexType name="DocumentInfoType">
		<xsd:annotation>
		    <xsd:documentation>
			This class contains elements related to information about a document. 
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Kind" type="PublicDocuments:DocumentKindChoice" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property refers to the class DocumentKindChoice.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			<xsd:element name="IssueDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation>
						This property contains the issue date of the document.
				
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
			
			
				
				
				    <xsd:element name="ReferenceNumber" minOccurs="1" maxOccurs="1" type="ReferenceNumberType"/>
				
				    
				
				    
				
				    
		</xsd:sequence>
		
	</xsd:complexType>
	
	
	
	
				
				    
				
				    <xsd:complexType name="DomicileAndOrResidenceType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the domicile and/or residence of a person.
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="ConcernedPerson" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Domicile" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Residence" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:complexType name="ExtractFromTheCivilStatusRegisterType"/>
				
				    
				
				    
				
				    
	
				
				    <xsd:complexType name="ExtractFromThePopulationRegisterType"/>
				
				    
				
				    
				
				    
	
				
				    
				
				    <xsd:complexType name="FormHeaderType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form header of a form.
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Country" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="AuthorityIssuingForm" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="AuthorityIssuingDocument" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="DocumentInfo" type="DocumentInfoType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="SignatureInfo" type="SignatureInfoType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="ExtensionHeader" type="ext:UBLExtensionsType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains potential future extensions to the header of each form. 
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="Title" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains the title of the form, corresponding to the main subject or life area covered in the form. 
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="ImportantNotice" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains a notice on the purpose of the form. 
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="NoteForTheIssuingAuthority" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains a note for the issuing authority regarding which information is to be mentioned. 
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		
				
				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmAbsenceOfCriminalRecord" type="FrmAbsenceOfCriminalRecordType"/>
				
				    
				
				    
				
