				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmDomicileAndOrResidence" type="FrmDomicileAndOrResidenceType"/>
				
				    
				
				    
				
				    <xsd:complexType name="FrmDomicileAndOrResidenceType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form DomicileAndOrResidence.
				
				    				Stereotype xsdGlobalElement
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Header" type="FormHeaderType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Body" type="DomicileAndOrResidenceType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmLife" type="FrmLifeType"/>
				
				    
				
				    
				
				    <xsd:complexType name="FrmLifeType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form Life.
				
				    				Stereotype xsdGlobalElement
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Header" type="FormHeaderType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Body" type="LifeType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmMaritalStatus" type="FrmMaritalStatusType"/>
				
				    
				
				    
				
				    <xsd:complexType name="FrmMaritalStatusType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form MaritalStatus.
				
				    				Stereotype xsdGlobalElement
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Header" type="FormHeaderType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Body" type="MaritalStatusType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
