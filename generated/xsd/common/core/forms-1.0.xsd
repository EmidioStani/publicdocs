				    <xsd:complexType name="IDTypeAndNoType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the choice between the types of ID used to identify a person.
				
				    				Stereotype xsdChoice
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:choice>
				
				    		<xsd:element name="NationalIdentityCardNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains the string value of the number of the national identity card.
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="PassportNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains the string value of the passport number.
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="DrivingLicenceNo" type="udt:TextType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains the string value of the driving licence number.
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
				    		</xsd:element>
				
				    		
				
				    		
				
				    		
				
				    		<xsd:element name="OtherIDType" type="OtherIDTypeType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
				    	</xsd:choice>
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    
				
				    <xsd:complexType name="LifeType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the life of a person.
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Address" type="pdc:AddressType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="IsAlive" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xsd:annotation>
				
				    			    <xsd:documentation>
				
				    					This property contains the boolean value (true or false) for whether the person is alive.
				
				    			    </xsd:documentation>
				
				    			</xsd:annotation>
				
