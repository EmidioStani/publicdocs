				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmCapacityToMarry" type="FrmCapacityToMarryType"/>
				
				    
				
				    
				
				    <xsd:complexType name="FrmCapacityToMarryType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form CapacityToMarry.
				
				    				Stereotype xsdGlobalElement
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Header" type="FormHeaderType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		<xsd:element name="Body" type="CapacityToMarryType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
				    		
				
				    	</xsd:sequence>
				
				    	
				
				    </xsd:complexType>
				
				    
				
				    
				
				    
	
				
				    <xsd:element name="FrmDeath" type="FrmDeathType"/>
				
				    
				
				    
				
				    <xsd:complexType name="FrmDeathType">
				
				    	<xsd:annotation>
				
				    	    <xsd:documentation>
				
				    		This class contains elements related to the form Death. 
				
				    				Stereotype xsdGlobalElement
				
				    	    </xsd:documentation>
				
				    	</xsd:annotation>
				
				    	<xsd:sequence>
				
				    		<xsd:element name="Header" type="FormHeaderType" minOccurs="1" maxOccurs="1"/>
				
				    		
				
				    		
				
