	<!-- ===================================================== -->
	<xsd:complexType name="AbsenceOfCriminalRecordPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the absence of a criminal record for a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					
					
					<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonPlaceOfBirthCoreLocation" minOccurs="1" maxOccurs="1"/>
								<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="1" maxOccurs="1"/>
								
													<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														<xsd:annotation>
														    <xsd:documentation xml:lang="en">
																The value of this property is of the class Sex.
														    </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													
					
					
					<xsd:element ref="cva:PersonCitizenshipJurisdiction" minOccurs="1" maxOccurs="unbounded"/>
										<xsd:element name="PreviousSurname" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
											<xsd:annotation>
											    <xsd:documentation xml:lang="en">
													This property contains the previous surname(s) of the person.
											    </xsd:documentation>
											</xsd:annotation>
										</xsd:element>	
										
										<xsd:element name="PreviousForename" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
											<xsd:annotation>
											    <xsd:documentation xml:lang="en">
													This property contains the previous forename(s) of the person.
											    </xsd:documentation>
											</xsd:annotation>
										</xsd:element>	
										
					
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="AddressType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to an address.
					<!-- Dependency Address->Address -->
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			
			
			<xsd:element ref="cvb:AddressThoroughfare" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element ref="cvb:AddressPostName" minOccurs="1" maxOccurs="1"/>
			
			
			<xsd:element ref="cvb:AddressAdminUnitLocationOne" minOccurs="1" maxOccurs="1"/>
								<xsd:element name="PostalCode" type="udt:TextType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											This property contains the postal code of the address
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="AuthorityType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the designation of an authority.
					<!-- Dependency Authority->LegalEntity -->
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			
			
			<xsd:element ref="cvb:LegalEntityLegalName" minOccurs="1" maxOccurs="1"/>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="BirthPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the birth of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					
					
					<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonPlaceOfBirthCoreLocation" minOccurs="1" maxOccurs="1"/>
								<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="1" maxOccurs="1"/>
								
													<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														<xsd:annotation>
														    <xsd:documentation xml:lang="en">
																The value of this property is of the class Sex.
														    </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													
					
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CanMarryType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the capacity to marry of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
								<xsd:element name="HasCapacityToMarry" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											The person concerned has the capacity to marry under the national law of the Member State where the public document is issued.
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
								<xsd:element name="AppearsToHaveNoMarriage" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											The person concerned appears to have no marriage in his or her name under the national law of the Member State where the public document is issued.
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
								<xsd:element name="NoKnownImpediment" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											There is no known impediment for the person concerned to marry under the national law of the Member State where the public document is issued.
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
								<xsd:element name="NoKnownImpedimentIntendedSpouse" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											There is no known impediment to a marriage between the person concerned and his or her intended spouse under the national law of the Member State where the public document is issued.
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
								<xsd:element name="NoOppositionknown" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
									<xsd:annotation>
									    <xsd:documentation xml:lang="en">
											There is no opposition known to a marriage between the person concerned and his or her intended spouse under the national law of the Member State where the public document is issued.
									    </xsd:documentation>
									</xsd:annotation>
								</xsd:element>
								
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="CapacityToMarryOrEnterRegisteredPartnershipPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the capacity to marry or enter into a registered partnership of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					
					
					<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonPlaceOfBirthCoreLocation" minOccurs="1" maxOccurs="1"/>
								<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="1" maxOccurs="1"/>
								
													<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														<xsd:annotation>
														    <xsd:documentation xml:lang="en">
																The value of this property is of the class Sex.
														    </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													
					
					
					<xsd:element ref="cva:PersonCitizenshipJurisdiction" minOccurs="0" maxOccurs="unbounded"/>
					
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="CountryType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to a country.
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
						<xsd:element name="Code" type="ids5ISO316612A:ISOTwoletterCountryCodeIdentifierContentType" minOccurs="0" maxOccurs="1" >
							<xsd:annotation>
							    <xsd:documentation xml:lang="en">
									The value of this property is of the class CountryCode.
							    </xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						
			
			
			<xsd:element ref="cvb:LocationGeographicName" minOccurs="1" maxOccurs="1"/>
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="DeathPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the death of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					
					
					<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonPlaceOfBirthCoreLocation" minOccurs="1" maxOccurs="1"/>
								<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="1" maxOccurs="1"/>
								
					
					
					<xsd:element ref="cvb:PersonDeathDate" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonPlaceOfDeathCoreLocation" minOccurs="1" maxOccurs="1"/>
					
					
					<xsd:element ref="cva:PersonCountryOfDeathCoreLocation" minOccurs="1" maxOccurs="1"/>
													<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														<xsd:annotation>
														    <xsd:documentation xml:lang="en">
																The value of this property is of the class Sex.
														
														    </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													
					
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="MaritalOrRegisteredPartnershipStatusPersonType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to the marital or registered partnership status of a person.
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:complexContent>
														
														    		<xsd:restriction base="PersonType">
														
														    			<xsd:sequence>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonBirthName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonGivenName" minOccurs="0" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cva:PersonPlaceOfBirthCoreLocation" minOccurs="1" maxOccurs="1"/>
														
														    							<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="1" maxOccurs="1"/>
														
														    							
														
														    												<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														
														    													<xsd:annotation>
														
														    													    <xsd:documentation xml:lang="en">
														
														    															The value of this property is of the class Sex.
														
														    													    </xsd:documentation>
														
														    													</xsd:annotation>
														
														    												</xsd:element>
														
														    												
														
														    				
														
														    				
														
														    				<xsd:element ref="cva:PersonCitizenshipJurisdiction" minOccurs="1" maxOccurs="unbounded"/>
														
														    									<xsd:element name="SurnameBefore" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
														
														    										<xsd:annotation>
														
														    										    <xsd:documentation xml:lang="en">
														
														    												This property contains the previous surname of the person.
														
														    										    </xsd:documentation>
														
														    										</xsd:annotation>
														
														    									</xsd:element>	
														
														    									
														
														    									<xsd:element name="SurnameAfter" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
														
														    										<xsd:annotation>
														
														    										    <xsd:documentation xml:lang="en">
														
														    												This property contains the future surname of the person.
														
														    										    </xsd:documentation>
														
														    										</xsd:annotation>
														
														    									</xsd:element>	
														
														    									
														
														    				
														
														    			</xsd:sequence>
														
														    		</xsd:restriction>
														
														    	</xsd:complexContent>
														
														    </xsd:complexType>
														
														    <xsd:complexType name="MaritalStatusChoiceType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to the choice of marital status.
														
														    				Stereotype xsdChoice
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:choice>
														
														    							<xsd:element name="DateOfMarriageOrRegisteredPartnership" type="udt:DateType" minOccurs="1" maxOccurs="1" >
														
														    								<xsd:annotation>
														
														    								    <xsd:documentation xml:lang="en">
														
														    										This property contains the date of marriage.
														
														    								    </xsd:documentation>
														
														    								</xsd:annotation>
														
														    							</xsd:element>
														
														    							
														
														    				<xsd:element name="NotMarried" minOccurs="1" maxOccurs="1" type="NotMarriedChoiceType">
														
														    				</xsd:element>
														
														    				
														
														    							<xsd:element name="Undetermined" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
														
														    								<xsd:annotation>
														
														    								    <xsd:documentation xml:lang="en">
														
														    										The marital status of the concerned person is undetermined.
														
														    								    </xsd:documentation>
														
														    								</xsd:annotation>
														
														    							</xsd:element>
														
														    							
														
														    		
														
														    	</xsd:choice>
														
														    </xsd:complexType>
														
														    <xsd:complexType name="MarriageOrRegisteredPartnershipPersonType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to the marriage or registered partnership of a person.
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:complexContent>
														
														    		<xsd:restriction base="PersonType">
														
														    			<xsd:sequence>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonBirthName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonBirthDate" minOccurs="1" maxOccurs="1"/>
														
														    												<xsd:element name="Sex" type="cls:SexType" minOccurs="1" maxOccurs="1" >
														
														    													<xsd:annotation>
														
														    													    <xsd:documentation xml:lang="en">
														
														    															The value of this property is of the class Sex.
														
														    													    </xsd:documentation>
														
														    													</xsd:annotation>
														
														    												</xsd:element>
														
														    												
														
														    									<xsd:element name="SurnameBefore" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
														
														    										<xsd:annotation>
														
														    										    <xsd:documentation xml:lang="en">
														
														    												This property contains the previous surname(s) of the person.
														
														    										    </xsd:documentation>
														
														    										</xsd:annotation>
														
														    									</xsd:element>	
														
														    									
														
														    									<xsd:element name="SurnameAfter" type="udt:TextType" minOccurs="1" maxOccurs="unbounded">
														
														    										<xsd:annotation>
														
														    										    <xsd:documentation xml:lang="en">
														
														    												This property contains the future surname(s) of the person.
														
														    										    </xsd:documentation>
														
														    										</xsd:annotation>
														
														    									</xsd:element>	
														
														    									
														
														    				
														
														    			</xsd:sequence>
														
														    		</xsd:restriction>
														
														    	</xsd:complexContent>
														
														    </xsd:complexType>
														
														    <xsd:complexType name="NationalityType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to a nationality.
														
														    				<!-- Dependency Nationality->Jurisdiction -->
														
														    				Stereotype cvABIE
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:sequence> 
														
														    		
														
														    		
														
														    		<xsd:element ref="cvb:JurisdictionName" minOccurs="1" maxOccurs="1"/>
														
														    		
														
														    	</xsd:sequence>
														
														    	
														
														    </xsd:complexType>
														
														    <xsd:complexType name="NotMarriedChoiceType">
														
