	<xsd:complexType name="NotMarriedChoiceType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the specific situation of not-married individuals.
					Stereotype xsdChoice
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="NeverMarried" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						The concerned person has never been married.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DivorceDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the date of the divorce.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WidowshipDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the date of widowship.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="OfficialType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					<xsd:element name="Surname" type="cvb:PersonFamilyNameType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
								cvb:PersonFamilyName
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="Forename" type="cvb:PersonGivenNameType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
								cvb:PersonGivenName
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to a person.
					<!-- Dependency Person->Person -->
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Surname" type="cvb:PersonFamilyNameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the person's last name.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="SurnameAtBirth" type="cvb:PersonBirthNameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the person's last name at the time of his/her birth.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="Forename" type="cvb:PersonGivenNameType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the person's first name or given name.
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="DateOfBirth" type="cvb:PersonBirthDateType" minOccurs="0" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the person's date of birth.
				
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceOfBirth" type="cva:CoreLocationType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="CountryOfBirth" type="CountryType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="DateOfDeath" type="cvb:PersonDeathDateType" minOccurs="0" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the person's date of death (if applicable).
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceOfDeath" type="cva:CoreLocationType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="CountryOfDeath" type="cva:CoreLocationType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Sex" type="cls:SexType" minOccurs="0" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the sex of the person
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Nationality" type="cva:JurisdictionType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Domicile" type="AddressType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="Residence" type="AddressType" minOccurs="0" maxOccurs="1"/>
			<xsd:element name="SurnameBefore" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the past surname of the person (if applicable)
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="SurnameAfter" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the future surname of the person (if applicable)
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="PreviousSurname" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the previous surname of the person (if applicable)
				
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			<xsd:element name="PreviousForename" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the previous forename (first name) of the person (if applicable). 
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>	
			
		</xsd:sequence>
		
	</xsd:complexType>
	<xsd:complexType name="PlaceType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to a place.
					<!-- Dependency Place->Location -->
					Stereotype cvABIE
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence> 
			<xsd:element name="Name" type="cvb:LocationGeographicNameType" minOccurs="1" maxOccurs="1" >
				<xsd:annotation>
				    <xsd:documentation xml:lang="en">
						This property contains the name of the place (geographic location).
				    </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			
		</xsd:sequence>
		
	</xsd:complexType>
	
</xsd:schema>

