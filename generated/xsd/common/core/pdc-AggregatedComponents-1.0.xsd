	<xsd:complexType name="MaritalOrRegisteredPartnershipStatusPersonType">
		<xsd:annotation>
		    <xsd:documentation xml:lang="en">
			This class contains elements related to the marital or registered partnership status of a person.
		    </xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="PersonType">
				<xsd:sequence>
					<xsd:element name="Surname" type="cvb:PersonFamilyNameType" minOccurs="1" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
