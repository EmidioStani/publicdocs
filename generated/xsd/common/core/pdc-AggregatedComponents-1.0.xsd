														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to the specific situation of not-married individuals.
														
														    				Stereotype xsdChoice
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:choice>
														
														    							<xsd:element name="NeverMarried" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
														
														    								<xsd:annotation>
														
														    								    <xsd:documentation xml:lang="en">
														
														    										The concerned person has never been married.
														
														    								    </xsd:documentation>
														
														    								</xsd:annotation>
														
														    							</xsd:element>
														
														    							
														
														    							<xsd:element name="DivorceDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
														
														    								<xsd:annotation>
														
														    								    <xsd:documentation xml:lang="en">
														
														    										This property contains the date of the divorce.
														
														    								    </xsd:documentation>
														
														    								</xsd:annotation>
														
														    							</xsd:element>
														
														    							
														
														    							<xsd:element name="WidowshipDate" type="udt:DateType" minOccurs="1" maxOccurs="1" >
														
														    								<xsd:annotation>
														
														    								    <xsd:documentation xml:lang="en">
														
														    										This property contains the date of widowship.
														
														    								    </xsd:documentation>
														
														    								</xsd:annotation>
														
														    							</xsd:element>
														
														    							
														
														    		
														
														    	</xsd:choice>
														
														    </xsd:complexType>
														
														    <xsd:complexType name="OfficialType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:complexContent>
														
														    		<xsd:restriction base="PersonType">
														
														    			<xsd:sequence>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonFamilyName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    				
														
														    				<xsd:element ref="cvb:PersonGivenName" minOccurs="1" maxOccurs="unbounded"/>
														
														    				
														
														    			</xsd:sequence>
														
														    		</xsd:restriction>
														
														    	</xsd:complexContent>
														
														    </xsd:complexType>
														
														    <xsd:complexType name="PersonType">
														
														    	<xsd:annotation>
														
														    	    <xsd:documentation xml:lang="en">
														
														    		This class contains elements related to a person.
														
														    				<!-- Dependency Person->Person -->
														
														    				Stereotype cvABIE
														
														    	    </xsd:documentation>
														
														    	</xsd:annotation>
														
														    	<xsd:sequence> 
														
														    		
														
														    		
														
														    		<xsd:element ref="cvb:PersonFamilyName" minOccurs="0" maxOccurs="unbounded"/>
														
														    		
														
														    		
														
														    		<xsd:element ref="cvb:PersonBirthName" minOccurs="0" maxOccurs="unbounded"/>
														
														    		
														
														    		
														
														    		<xsd:element ref="cvb:PersonGivenName" minOccurs="0" maxOccurs="unbounded"/>
														
														    		
														
														    		
														
														    		<xsd:element ref="cvb:PersonBirthDate" minOccurs="0" maxOccurs="1"/>
														
														    		
														
														    		
														
