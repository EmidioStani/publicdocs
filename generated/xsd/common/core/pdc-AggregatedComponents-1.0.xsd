						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
								This property contains the person's surname at the time of his/her birth.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="Forename" type="cvb:PersonGivenNameType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
								This property contains the person's forename (i.e. first name or given name).
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>	
					<xsd:element name="DateOfBirth" type="cvb:PersonBirthDateType" minOccurs="1" maxOccurs="1" >
						<xsd:annotation>
						    <xsd:documentation xml:lang="en">
								This property contains the person's date of birth.
						    </xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="PlaceOfBirth" type="cva:CoreLocationType" minOccurs="1" maxOccurs="1"/>
