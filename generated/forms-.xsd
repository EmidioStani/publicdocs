		<:sequence> 
			<:element name="SpouseA" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="SpouseB" type="pdc:PersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="PlaceOfMarriage" type="pdc:PlaceType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="OtherIDTypeType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to ID types other than national identity cards, passports, or driving licences.
		    </:documentation>
		</:annotation>
		<:sequence> 
			
		</:sequence>
		
	</:complexType>
	<:complexType name="RegisteredPartnershipType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to a registered partnership.
		    </:documentation>
		</:annotation>
