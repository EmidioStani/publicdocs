	<:complexType name="AbsenceOfCriminalRecordType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the absence of a criminal record of a person. 
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:AbsenceOfCriminalRecordPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="IDTypeAndNo" minOccurs="1" maxOccurs="1" type="IDTypeAndNoType">
			</:element>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="AdministrativeDocumentChoiceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the choice between the different types of administrative documents. 
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			
		</:choice>
	</:complexType>
	<:complexType name="BirthType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the birth of a person.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="CapacityToEnterRegisteredPartnershipType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the capacity to enter into a registered partnership of a couple.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="IntendedPartner" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="CanEnterRegisteredPartnership" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="CapacityToMarryType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the capacity to marry of a couple.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="IntendedSpouse" type="pdc:CapacityToMarryOrEnterRegisteredPartnershipPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="CanMarry" type="pdc:CanMarryType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="DeathType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the death of a person.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:DeathPersonType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="DocumentFromCourtOrTribunalChoiceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the choice between different types of documents emanating from a court or tribunal.
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			
		</:choice>
	</:complexType>
	<:complexType name="DocumentInfoType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to information about a document. 
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Kind" minOccurs="1" maxOccurs="1" type="DocumentKindChoiceType">
			</:element>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="DocumentKindChoiceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the choice between different types of documents.
					Stereotype xsdChoice
		    </:documentation>
		</:annotation>
		<:choice>
			<:element name="AdministrativeDocument" minOccurs="1" maxOccurs="1" type="AdministrativeDocumentChoiceType">
			</:element>
			
		</:choice>
	</:complexType>
	<:complexType name="DomicileAndOrResidenceType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the domicile and/or residence of a person.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="ConcernedPerson" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
			<:element name="Domicile" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
			<:element name="Residence" type="pdc:AddressType" minOccurs="0" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
	<:complexType name="FormHeaderType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the form header of a form.
		    </:documentation>
		</:annotation>
