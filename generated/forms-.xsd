			<:element name="Country" type="pdc:CountryType" minOccurs="1" maxOccurs="1"/>
			<:element name="AuthorityIssuingForm" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
			<:element name="AuthorityIssuingDocument" type="pdc:AuthorityType" minOccurs="1" maxOccurs="1"/>
			<:element name="DocumentInfo" type="DocumentInfoType" minOccurs="1" maxOccurs="1"/>
			<:element name="SignatureInfo" type="SignatureInfoType" minOccurs="1" maxOccurs="1"/>
			
		</:sequence>
		
	</:complexType>
