			<:element name="OtherIDType" type="OtherIDTypeType" minOccurs="1" maxOccurs="1"/>
			
		</:choice>
	</:complexType>
	<:complexType name="LifeType">
		<:annotation>
		    <:documentation xml:lang="">
			This class contains elements related to the life of a person.
		    </:documentation>
		</:annotation>
		<:sequence> 
			<:element name="Person" type="pdc:BirthPersonType" minOccurs="1" maxOccurs="1"/>
